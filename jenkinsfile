pipeline {
   agent any

   stages {
      stage('build') {
         steps {
            mvn clean install
         }
      }
      stage('test') {
         steps {
            mvn test
         }
      }
      stage('deploy dev') {
         steps {
            echo 'Hello World dev'
         }
      }
      stage('deploy test') {
         steps {
            echo 'Hello World test'
         }
      }
   }
}
