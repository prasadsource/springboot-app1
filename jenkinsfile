pipeline {
   agent any

   stages {
      stage('build') {
         steps {
            //mvn clean install
            echo 'Hello World build'
            withMaven(maven:"maven-3.6.3"){
             bat 'mvn clean compile'
            }
         }
      }
      stage('test') {
         steps {
            //mvn test
            echo 'Hello World test'
            bat 'mvn test'
         }
      }
      stage('deploy dev') {
         steps {
            echo 'Hello World dev'
         }
      }
      stage('deploy test') {
         steps {
            echo 'Hello World test'
         }
      }
   }
}
